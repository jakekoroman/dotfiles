! Sets font
URxvt*font: xft:Iosevka Nerd Font Mono:size=11:antialias=true

! Removes stupid scrollbar
URxvt.scrollBar: false

! Restore 'normal' copy & paste funcitonality
! (Ctrl + Shift + C) - Copy
! (Ctrl + Shift + V) - Paste
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
! Disables other stupid (Ctrl + Shift) iso shit that is so dumb I can"'"t even comprehend
URxvt.iso14755: false
URxvt.iso14755_52: false

